{
  "manifest_version": 2,
  "name": "Athena",
  "description": "A dictionary lookup extension for both Myanmar and English languages",
  "version": "0.1",
  "permissions": [
    "http://www.ornagai.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "js/athena.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "fonts/fontello.woff"
  ],
  "author": "Kaung Htet Zaw"
}
